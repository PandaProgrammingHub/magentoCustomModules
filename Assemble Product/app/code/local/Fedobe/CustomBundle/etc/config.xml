<?xml version="1.0" encoding="UTF-8"?>
<config>
    <modules>
        <Fedobe_CustomBundle>
            <version>0.1</version>
        </Fedobe_CustomBundle>
    </modules>
    <global>
        <helpers>
          <custombundle>
		<class>Fedobe_CustomBundle_Helper</class>
	  </custombundle>
       </helpers>
        <models>
            <custombundle>
                <class>Fedobe_CustomBundle_Model</class>
            </custombundle>
        </models>
        <blocks>
             <custombundle>
                <class>Fedobe_CustomBundle_Block</class>
            </custombundle>
           <!-- <bundle>
                <rewrite>
                    <catalog_product_view_type_bundle>
                        Fedobe_Catalog_Product_View_Type_Bundle
                    </catalog_product_view_type_bundle>
                </rewrite>
            </bundle>-->
        </blocks>
        <catalog>
            <product>
                <options>
                    <bundle>
                        <types>
                            <nav translate="label" module="bundle">
                                <label>Navigation Menu</label>
                            </nav>
                        </types>
                    </bundle> 
                </options> 
            </product>
        </catalog>
    </global>
    <frontend>
        <layout>
            <updates>
                <custombundle>
                    <file>fedobe/custombundle.xml</file>
                </custombundle>
            </updates>
        </layout>
    </frontend>
</config>